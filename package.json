    "coretest": "mocha test/poetifier.tests.js",
    "test": "standard --fix && npm run test:js",
    "test:js": "mocha --timeout 50000 --recursive ./test/*.tests.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelPaulukonis/napogenmo2016"
  },
  "author": "Michael Paulukonis",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "commander": "2.9.0",
    "common-corpus": "github:michaelpaulukonis/common-corpus",
    "dotenv": "2.0.0",
    "fuzzy-matching": "0.4.3",
    "htmlencode": "0.0.4",
    "iconv-lite": "0.4.13",
    "in-a-nutshell": "0.1.2",
    "nlp-ngram": "0.0.3",
    "nlp-pronounce": "0.0.1",
    "nlp_compromise": "6.1.4",
    "node-mispelr": "0.0.1",
    "pos": "~0.1.7-2",
    "random-seed": "0.3.0",
    "rhymes": "^1.0.2",
    "simple-timer": "0.0.5",
    "tagspewer": "0.3.1",
    "tumblrwks": "0.3.0"
  },
  "engines": {
    "node": "6.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dirty-chai": "^2.0.1",
    "gulp": "3.9.1",
    "gulp-eslint": "2.0.0",
    "gulp-if": "2.0.1",
    "gulp-mocha": "2.2.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "simple-timer": "0.0.5",
    "standard": "^12.0.1"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after"
    ]
